	</resultMap>
	<update id="updateIntroduce" parameterType="com.actibuddy.member.model.dto.MemberDTO">
			
			UPDATE MEMBER A
			   SET A.INTRODUCE = #{ introduce }
			     , A.FAVORITE_ACTI = #{ favoriteActi }
		 	 WHERE A.USER_ID = #{ userId }
	</update>
	
	<update id="changeUseYn" parameterType="com.actibuddy.member.model.dto.MemberDTO">
	
		UPDATE PAY_HIS P
   		   SET USE_YN = 'Y'
   		 WHERE CART_NUM = #{cartNum};
 
 	</update>
 	
 	<select id="selectTripList" parameterType="hashMap" resultMap="tripListResultMap" >
 	
			 SELECT
			        A.ACTI_NAME
			      , A.ACTI_NUM
			      , A.ACTI_IMG
			      , C.CHOOSE_DATE
			      , C.TOTAL_PERSON
			      , C.CHOOSE_OPTION
			      , C.CART_NUM
			      , P.USE_YN
			      , M.USER_ID
			      , M.INTRODUCE
			      , M.FAVORITE_ACTI
                  , P.PAY_STATUS
			  FROM CART C
			 INNER JOIN ACTIVITY_INFO A
			    ON A.ACTI_NUM = C.ACTI_NUM
			 INNER JOIN PAY_HIS P
			    ON C.CART_NUM = P.CART_NUM
			 INNER JOIN MEMBER M
			    ON C.USER_ID = M.USER_ID
			 WHERE M.USER_ID = #{ userId }
			 <if test="actiName != null">
			   AND A.ACTI_NAME = #{ actiName }
			 </if>
               AND P.PAY_STATUS = 'Y'
 	
 	</select>
 	
 	<select id="selectCart" parameterType="String" resultMap="tripListResultMap">
 		
 		SELECT
			        A.ACTI_NAME
			      , A.ACTI_IMG  
			      , A.ACTI_NUM
			      , C.CHOOSE_DATE
			      , C.TOTAL_PERSON
			      , C.CHOOSE_OPTION
			      , C.TOTAL_PRICE
			      , C.CART_NUM
			  FROM CART C
			 INNER JOIN ACTIVITY_INFO A
			    ON A.ACTI_NUM = C.ACTI_NUM
			 INNER JOIN MEMBER M
			    ON C.USER_ID = M.USER_ID
			 WHERE M.USER_ID = #{ userId }
			   AND C.PAY_YN = 'N'
 	
 	</select>

    <insert id="insertCart">
    
        INSERT
          INTO CART A
        (
           A.ACTI_NUM
         , A.CART_NUM
         , A.USER_ID
         , A.CHOOSE_OPTION
         , A.TOTAL_PERSON
         , A.TOTAL_PRICE
         , A.CHOOSE_DATE
         , A.PAY_YN
        )
        VALUES
        (
          #{ actiNum }
        , ('CART' || TO_CHAR(SEQ_CART_NUM.nextval))
        , #{ userId }
        , #{ chooseOption }
        , #{ totalPerson }
        , #{ totalPrice }
        , #{ chooseDate }
        , DEFAULT
        )
    
    </insert>
    
    <select id="selectMtReview" resultMap="mateReviewResultMap">
    	SELECT
		        M.USER_ID
		      , R.MT_REVIEW_TITLE
		      , R.MT_REVIEW_NUM
		  FROM  MT_REVIEW R
		  INNER JOIN MEMBER M
		     ON R.USER_ID = M.USER_ID
		  WHERE M.USER_ID=#{ userId }
    
    
    </select>
    
	 <select id="selectActiInfo" resultMap="activityResultMap">
    	SELECT
	    		 A.ACTI_NUM
				,A.ACTI_NAME
				,A.LOCATION
				,A.PRICE
				,A.START_DATE
				,A.END_DATE
				,A.LOCATION_CODE
				,A.ACTI_TYPE_NUM
				,A.USER_ID
		FROM ACTIVITY_INFO A
	   WHERE A.USER_ID = #{ userId }

    </select>
    
    <select id="selectActiReview" resultMap="activityAndReviewMap">
     
      SELECT
      		      AR.ACTI_REVIEW_NUM
				, AR.ACTI_REVIEW_DATE
				, AR.ACTI_NUM
				, AR.WRITER_ID
				, AR.ACTI_REVIEW_TITLE
				, AR.ACTI_REVIEW_CON
				, AR.ACTI_REVIEW_STAR
				, AR.ACTI_REVIEW_IMG
				, AR.ACTI_REVIEW_REC
				, AR.ACTI_REVIEW_REP_YN
                , A.ACTI_NAME
	    FROM ACTI_REVIEW AR
        INNER JOIN ACTIVITY_INFO A
			    ON A.ACTI_NUM = AR.ACTI_NUM
	   WHERE AR.WRITER_ID = #{ userId }
	     AND AR.ACTI_REVIEW_TITLE IS NOT NULL
    </select>

	<update id="updateQuit">
	
		UPDATE MEMBER A
			   SET A.QUIT_YN = 'Y'
		 	 WHERE A.USER_ID = #{ userId }
	
	</update>
	
	<select id="selectMateScore" resultMap="mateScoreResultMap">
		SELECT 
				SCORED_ID
			  , MT_SCORE_TITLE
			  , MT_SCORE_CON
			  , MT_SCORE_REP_YN
			  , USER_ID
			  , MT_SCORE_NUM
			  , MT_SCORE_STAR
			  , PLUS_SCORE
		 FROM MT_SCORE
		WHERE SCORED_ID = #{ userId }
	</select>
	
	<delete id="deleteCart">
		DELETE 
		  FROM CART
		WHERE USER_ID = #{ userId }
		  AND CART_NUM = #{ cartNum }
	</delete>
	
	
	<insert id="insertReview">
		INSERT
          INTO ACTI_REVIEW A
        (
              A.ACTI_REVIEW_NUM
			, A.ACTI_REVIEW_DATE
			, A.ACTI_NUM
			, A.WRITER_ID
			, A.ACTI_REVIEW_TITLE
			, A.ACTI_REVIEW_CON
			, A.ACTI_REVIEW_STAR
			, A.ACTI_REVIEW_IMG
			, A.ACTI_REVIEW_REC
			, A.ACTI_REVIEW_REP_YN
        )
        VALUES
        (
         'ACTI_REVIEW_NUM' || TO_CHAR(SEQ_ACTI_REVIEW_NUM.NEXTVAL)
        ,  default
        , #{ actiNum }
        , #{ writerId }
        , #{ title }
        , #{ content }
        , #{ reviewStar }
        , '../resources/image/tripReview/' || #{ image }
        , 0 
        , 'N'
        )
	</insert>
	
	<insert id="insertPayHis">
	    INSERT
		  INTO PAY_HIS
		(
		  PAY_NUM
		, CART_NUM
		, PAY_STATUS
		, USE_YN
		)
		VALUES
		(
		  #{ payNum }
		, #{ cartNum }
		, 'Y'
		, DEFAULT
		)
	</insert>
	
	<select id="selectPayResult" resultMap="payResultMap" parameterType="String">
	    SELECT
               A.*
             , B.ACTI_NAME
             , B.ACTI_IMG
          FROM CART A
          JOIN ACTIVITY_INFO B ON (A.ACTI_NUM = B.ACTI_NUM)
         WHERE A.CART_NUM = #{ cartNum }
    </select>
    
	<select id="selectFaq" resultMap="faqResultMap">
		SELECT
			    QUE_NUM
			  ,	QUE_CON
			  ,	QUE_TITLE
			  ,	QUE_DATE
			  ,	USER_ID
			  ,	QUE_TYPE
			  ,	QUE_EMAIL
			  ,	ANSWER
			  ,	ANSWER_YN
		 FROM FAQ
		WHERE USER_ID = #{userId}
	</select>
	
	<insert id="insertActi">
	  INSERT
		INTO ACTIVITY_INFO
		(
		  ACTI_TYPE_NUM
		, USER_ID
	    , ACTI_NUM
		, ACTI_NAME
		, LOCATION
		, PRICE
		, START_DATE
		, END_DATE
		, ACTI_TIP
		, ACTI_IMG
		, ACTI_IMG2
		, ACTI_IMG3
		, LOCATION_CODE
		, MAIN_IMG
		, ACTI_NO
		)
		VALUES
		(
		  #{ activitTypeCode }
		, #{ userId }
		, SEQ_ACTI_NUM.NEXTVAL
		, #{ name }
		, #{ location }
		, #{ price }
		, #{ StartDate }
		, #{ EndDate }
		, #{ tip }
		, '../resources/image/addActi/' || #{ image }
		, '../resources/image/addActi/' || #{ image2 }
		, '../resources/image/addActi/' || #{ image3 }
		, #{ loactionCode } 
		, '/resources/image/addActi/' || #{ image }
		, SEQ_ACTI_NO.NEXTVAL
		)
	
	</insert>
	
	<select id="selectNewActiNum" resultMap="newActiResultMap">
		SELECT
		       RNUM
		     , V2.*
		  FROM (SELECT ROWNUM RNUM
		             , V.*
		          FROM (SELECT *
		                  FROM ACTIVITY_INFO
		                 ORDER BY ACTI_NO DESC
		               ) V
		       )V2
		  WHERE RNUM <![CDATA[ < ]]> 2
	</select>
	
	<insert id="insertCon">
	 INSERT 
	  INTO ACTI_CONDITION_HIS
	(
	  ACTI_NUM
	, ACTI_CON_NUM
	)
	VALUES
	(
	  #{ actiNum }
	, #{ ActiConNum }
	)
	</insert>
	
	<insert id="insertOp">
	INSERT
	  INTO ACTI_OPTION
	(
	  OPTION_NUM
	, OPTION_PRICE
	, OPTION_NAME
	, ACTI_NUM
	
	)
	VALUES
	(
	  'ACTI_OPTION_NUM' || SEQ_OPTION_NUM.NEXTVAL
	 ,#{ price }
	 ,#{ optionName }
	 ,#{ actiNum }
	
	)
	</insert>
	
	<insert id="insertScore">
	    INSERT 
		  INTO MT_SCORE
		(
		  SCORED_ID
		, MT_SCORE_TITLE
		, MT_SCORE_CON
		, MT_SCORE_REP_YN
		, USER_ID
		, MT_SCORE_NUM
		, MT_SCORE_STAR
		, PLUS_SCORE
		, SCORE_NO
		)
		VALUES
		(
		  #{ scoredId }
		, #{ scoreTitle }
		, #{ scoreCon }
		, 'N'
		, #{ userId }
		, ('MT_SCORE' || TO_CHAR(SEQ_MT_SCORE_NUM.nextval))
		, #{ scoreStar }
		, #{ plusScore }
		, SEQ_SCORE_NO.NEXTVAL
		)
	</insert>
	<select id="selectMtApply" resultMap="mtFindApplyResultMap">
          SELECT
		         A.MT_FIND_NUM
			   , A.MT_FIND_YN
			   , A.MT_APPLY_NUM
			   , A.APPLIED_ID
			   , F.MT_FIND_TITLE
			   , F.MT_FIND_STATE
			   , A.MT_FIND_YN
	       FROM MT_APPLY_HIS A
          INNER JOIN MT_FIND F
             ON A.MT_FIND_NUM = F.MT_FIND_NUM
          WHERE F.USER_ID = #{ userId }
	</select>
	
	<update id="requestMtFindNum" parameterType="com.actibuddy.mate.model.dto.MateFindDTO">
		UPDATE MT_FIND
		   SET MT_FIND_STATE = '신청마감'
		 WHERE MT_FIND_NUM = #{ num }
	
	</update>
	
	<update id="updatePayYn">
	    UPDATE CART
		   SET PAY_YN = 'Y'
		 WHERE CART_NUM = #{ cartNum }
	
	</update>

    <select id="selectScoreNum" resultMap="mateScoreResultMap">
        SELECT RNUM
		     , B.*
		  FROM (SELECT ROWNUM RNUM
		     , A.*
		  FROM (SELECT *
		          FROM MT_SCORE
		         ORDER BY SCORE_NO DESC
		       )A
		  )B
		 WHERE RNUM <![CDATA[ < ]]> 2
    </select>
    
    <select id="selectAppNo" resultMap="mtApplyResultMap">
        SELECT *
		  FROM MT_APPLY_HIS
		 WHERE APPLIED_ID = #{ appliedId }
		   AND MT_FIND_NUM = #{ num }
    </select>
    
    <insert id="isertScoreHis">
        INSERT
		  INTO MT_SCORE_HIS
		(
		  MT_APPLY_NUM
		, MT_SCORE_NUM
		)
		VALUES
		(
		  #{ aNum }
		, #{ sNum }
		)
    </insert>
    
    <update id="updateFindStateYes">
    	UPDATE MT_APPLY_HIS
    	   SET MT_FIND_YN = 'Y'
    	 WHERE APPLIED_ID = #{appliedId}
    	   AND MT_FIND_NUM = #{mtFindNum}
    	  
    </update>
    
    <update id="updateFindStateNo">
    	UPDATE MT_APPLY_HIS
    	   SET MT_FIND_YN = 'X'
    	 WHERE APPLIED_ID = #{appliedId}
    	   AND MT_FIND_NUM = #{mtFindNum}
    
    </update>
    
    
  
  </mapper>